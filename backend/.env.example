# Server Configuration
PORT=3000
NODE_ENV=development

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=azure_automation
DB_USER=postgres
DB_PASSWORD=password

# InfluxDB Configuration
INFLUX_URL=http://localhost:8086
INFLUX_TOKEN=your-influx-token
INFLUX_ORG=my-org
INFLUX_BUCKET=azure-monitoring

# Azure Configuration
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id
AZURE_CLIENT_SECRET=your-client-secret
AZURE_SUBSCRIPTION_ID=your-subscription-id

# 企业微信通知配置
# Webhook URL - 从企业微信机器人获取
# WECOM_WEBHOOK_URL=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your-webhook-key

# 企业微信应用配置（可选，用于更复杂的通知）
# WECOM_CORP_ID=your-corp-id
# WECOM_AGENT_ID=your-agent-id
# WECOM_CORP_SECRET=your-corp-secret